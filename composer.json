{
    "name": "youlead-bow/vich-ftp-sync",
    "type": "symfony-bundle",
    "description": "Permet de synchroniser par FTP les fichiers attachés aux entités",
    "keywords": [
        "file sync",
        "ftp"
    ],
    "homepage": "https://github.com/youlead-bow/VichFtpSyncBundle",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "vich/uploader-bundle": "^2.4",
        "league/flysystem-ftp": "^3.29",
        "symfony/serializer": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Vich\\FtpSyncBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vich\\FtpSyncBundle\\Tests\\": "tests/"
        }
    }
}
